{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "declaration": true,
    "outDir": "./generated-client/dist",
    "rootDir": "./generated-client",
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "erasableSyntaxOnly": false
  },
  "include": ["./generated-client"],
  "exclude": ["./generated-client/dist"]
}
